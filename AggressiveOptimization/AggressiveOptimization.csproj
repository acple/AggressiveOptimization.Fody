<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DebugType>none</DebugType>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <DevelopmentDependency>true</DevelopmentDependency>
  </PropertyGroup>

  <PropertyGroup>
    <Version>0.0.1</Version>
    <Authors>acple</Authors>
    <Description>Fody add-in that disables Quick JIT for all your methods</Description>
    <Copyright>(c) 2020 acple</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/acple/AggressiveOptimization.Fody</PackageProjectUrl>
    <RepositoryUrl>https://github.com/acple/AggressiveOptimization.Fody.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>fody;jit</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Fody" Version="6.0.0" PrivateAssets="None" />
    <PackageReference Include="FodyPackaging" Version="6.2.6" PrivateAssets="All" />
  </ItemGroup>

</Project>
